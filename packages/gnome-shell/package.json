{
    "name": "@girs/gnome-shell",
    "version": "3.0.0",
    "description": "GJS TypeScript type definitions for GNOME Shell Extensions",

    "type": "module",
    "exports": {
        ".": {
            "import": {
                "types": "./src/index.d.ts",
                "default": "./src/index.js"
            },
            "require": {
                "types": "./src/index.d.ts",
                "default": "./src/index.cjs"
            }
        },
        "./misc/extensionUtils": {
            "import": {
                "types": "./src/misc/extensionUtils.d.ts",
                "default": "./src/misc/extensionUtils.js"
            },
            "require": {
                "types": "./src/misc/extensionUtils.d.ts",
                "default": "./src/misc/extensionUtils.cjs"
            }
        },
        "./extensions/extension": {
            "import": {
                "types": "./src/extensions/extension.d.ts",
                "default": "./src/extensions/extension.js"
            },
            "require": {
                "types": "./src/extensions/extension.d.ts",
                "default": "./src/extensions/extension.cjs"
            }
        },
        "./ui/main": {
            "import": {
                "types": "./src/ui/main.d.ts",
                "default": "./src/ui/main.js"
            },
            "require": {
                "types": "./src/ui/main.d.ts",
                "default": "./src/ui/main.cjs"
            }
        },
        "./ui/panelMenu": {
            "import": {
                "types": "./src/ui/panelMenu.d.ts",
                "default": "./src/ui/panelMenu.js"
            },
            "require": {
                "types": "./src/ui/panelMenu.d.ts",
                "default": "./src/ui/panelMenu.cjs"
            }
        },
        "./ui/popupMenu": {
            "import": {
                "types": "./src/ui/popupMenu.d.ts",
                "default": "./src/ui/popupMenu.js"
            },
            "require": {
                "types": "./src/ui/popupMenu.d.ts",
                "default": "./src/ui/popupMenu.cjs"
            }
        }
    },
    "typedoc": {
        "entryPoint": "./src/index.d.ts",
        "readmeFile": "./README.md",
        "displayName": "GNOME Shell",
        "tsconfig": "./tsconfig.doc.json"
    },
    "scripts": {
        "validate": "tsc --noEmit",
        "publish": "yarn npm publish --tolerate-republish --access public",
        "publish:latest": "yarn npm publish --tolerate-republish --tag latest --access public",
        "publish:next": "yarn npm publish --tolerate-republish --tag next --access public"
    },
    "dependencies": {
        "@girs/atk-1.0": "^2.50.0-3.2.4",
        "@girs/cally-13": "^13.0.0-3.2.4",
        "@girs/clutter-13": "^13.0.0-3.2.4",
        "@girs/cogl-2.0": "^2.0.0-3.2.4",
        "@girs/gcr-4": "^4.1.0-3.2.4",
        "@girs/gio-2.0": "^2.78.0-3.2.4",
        "@girs/gjs": "^3.2.4",
        "@girs/glib-2.0": "^2.78.0-3.2.4",
        "@girs/gnomebg-4.0": "^4.0.0-3.2.4",
        "@girs/gnomedesktop-4.0": "^4.0.0-3.2.4",
        "@girs/gobject-2.0": "^2.78.0-3.2.4",
        "@girs/meta-13": "^13.0.0-3.2.4",
        "@girs/shell-13": "^13.0.0-3.2.4",
        "@girs/shew-0": "^0.0.0-3.2.4",
        "@girs/st-13": "^13.0.0-3.2.4"
    },
    "devDependencies": {
        "typescript": "^5.2.2"
    },
    "keywords": [
        "Gir",
        "TypeScript",
        "types",
        "GObject-Introspection",
        "GJS",
        "GnomeShell",
        "GNOME",
        "Shell",
        "Extension"
    ],
    "author": "Pascal Garber <pascal@artandcode.studio>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gjsify/gnome-shell.git"
    },
    "bugs": {
        "url": "https://github.com/gjsify/gnome-shell/issues"
    },
    "homepage": "https://github.com/gjsify/gnome-shell#readme"
}
